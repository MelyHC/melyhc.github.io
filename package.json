    "jquery": "^3.7.1",
    "popper.js": "^1.16.1",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bootstrap": "^5.2.10",
    "eslint": "^9.39.2",
    "sass": "^1.77.6",
    "sass-embedded": "^1.97.1",
    "sass-loader": "^16.0.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"

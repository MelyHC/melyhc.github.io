{
  "extends": "astro/tsconfigs/base",
  "css": {
    "preprocessorOptions": {
      "scss": {
        "silenceDeprecations": ["mixed-decls", "color-functions", "global-builtin", "import"]
      },
    }
  },
   "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
